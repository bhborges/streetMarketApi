{
  "name": "fair-api",
  "version": "v0.0.1",
  "description": "Fairs API",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "npx babel-node server",
    "compile": "babel server --out-dir dist --delete-dir-on-start --source-maps inline --copy-files",
    "dev": "nodemon server --exec babel-node --config .nodemonrc.json | pino-pretty pino-tee warn ./warn-logs  >  ./all-logs",
    "dev:debug": "nodemon server --exec babel-node --config .nodemonrc.json --inspect | pino-pretty",
    "test:api": "mocha --timeout 10000 --require @babel/register --exit --reporter mochawesome --reporter-options reportDir=./__tests__/reports __tests__/api/*.js",
    "test": "DISABLE_REQUEST_AUTHORIZATION_HEADER=true nyc mocha --timeout 10000 --require @babel/register --exit ./test.js | pino-pretty",
    "lint": "eslint -c .eslintrc.json {server,test}/** --fix --no-error-on-unmatched-pattern",
    "migrate": "npx sequelize-cli db:migrate"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "babel-node": "^0.0.1-security",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "cpf-cnpj-validator": "^0.1.0",
    "crypto": "^1.0.1",
    "dotenv": "^8.1.0",
    "eslint-plugin-import-order-autofix": "^0.8.3",
    "express": "^4.17.1",
    "fs": "0.0.1-security",
    "i18n": "^0.8.4",
    "moment": "^2.24.0",
    "multer": "^1.4.2",
    "path": "^0.12.7",
    "pg": "^8.2.1",
    "pino": "^5.13.4",
    "redis": "^2.8.0",
    "request": "^2.88.0",
    "sequelize": "^5.21.2",
    "sequelize-cli": "^5.5.1",
    "swagger-ui-express": "^4.1.6",
    "xlsx": "^0.15.4",
    "yamljs": "^0.3.0",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.7.7",
    "@babel/node": "^7.12.10",
    "@babel/preset-env": "^7.6.2",
    "babel-eslint": "^10.0.3",
    "babel-plugin-istanbul": "^6.0.0",
    "chai": "^4.2.0",
    "chance": "^1.1.3",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-simple-import-sort": "^5.0.1",
    "husky": "^4.3.8",
    "istanbul": "^0.4.5",
    "mocha": "^6.2.2",
    "mochawesome": "^6.2.2",
    "nodemon": "^1.19.4",
    "nyc": "^15.1.0",
    "pino-pretty": "^3.2.1",
    "pino-tee": "^0.3.0",
    "prettier": "^1.18.2",
    "supertest": "^4.0.2"
  },
  "author": "Bruno Borges <borgesbruno@hotmail.com.br>"
}
